{
  "name": "X to XCancel Redirect",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Redirects x.com requests to xcancel.com",
  "background": {
    "service_worker": "service_worker.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules_1.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://x.com/*"
  ]
}